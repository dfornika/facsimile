<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE configuration [
    <!ATTLIST class id ID #IMPLIED>
    <!ATTLIST binding id ID #IMPLIED>
    <!ATTLIST input id ID #IMPLIED>
]>
<configuration>	
	<classBindings>
		<class id="class">http://purl.org/facsimile/dbq#DBQQuestion</class>
		<class id="sectionclass">http://purl.org/facsimile/dbq#DBQSection</class>
		<class id="outclass">http://purl.org/facsimile/dbq#DBQQuestion</class>
	</classBindings>		
	<propertyBindings>
		<!-- question property bindings -->
		<binding id="text">http://purl.org/facsimile/cfa#hasText</binding>
		<binding id="focus">http://purl.org/facsimile/cfa#hasFocus</binding>
		<binding id="value">http://purl.org/facsimile/cfa#hasValue</binding>
		<binding id="datavalue">http://purl.org/facsimile/cfa#hasDataValue</binding>
		<!-- section property bindings -->
		<binding id="sectionheading">http://purl.org/facsimile/cfa#hasHeading</binding>
		<binding id="sectionsortinglabel">http://purl.org/facsimile/cfa#hasSortingLabel</binding>
		<binding id="sectionquestion">http://purl.org/facsimile/datamodel#hasQuestion</binding>
	</propertyBindings>
	<questionTypeBindings>
		<input id="radio">http://purl.org/facsimile/cfa#BooleanValue</input>
		<input id="textfield">http://purl.org/facsimile/cfa#StringValue</input>
		<input id="checkbox"></input>
		<input id="dropdown"></input>
		<input id="combo"></input>
	</questionTypeBindings>	
	<form>
		<section>
			<questionList>
				<question>
					<iri>http://purl.org/facsimile/dbq#DBQ_Back_Q1A</iri>
				</question>
				<question>
					<iri>http://purl.org/facsimile/dbq#DBQ_Back_Q1B</iri>
				</question>
				<question>
					<iri>http://purl.org/facsimile/dbq#DBQ_Back_Q1C</iri>
				</question>
			</questionList>
		</section>
		<section>
			<questionList>
				<question>
					<iri>http://purl.org/facsimile/dbq#DBQ_Back_Q2</iri>
				</question>
			</questionList>
		</section>
		<section>
			<questionList>
				<question>
					<iri>http://purl.org/facsimile/dbq#DBQ_Back_Q3_1</iri>
				</question>
				<question>
					<iri>http://purl.org/facsimile/dbq#DBQ_Back_Q3_2</iri>
